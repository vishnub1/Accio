//Unit 2 | Data Types & Variables
//Celsius to Fahrenheit

import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
	    	int fer = (n*9/5)+32;
        System.out.println(fer);
    }
}
